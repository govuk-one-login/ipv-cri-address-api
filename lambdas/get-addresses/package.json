    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.92",
        "@types/jest": "^29.5.11",
        "@types/node": "18.8.3",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "aws-sdk": "2.1407.0",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
